{
  "name": "gafus.ru",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "analyze:trainer": "pnpm --filter @gafus/trainer-panel analyze",
    "analyze:web": "pnpm --filter @gafus/web analyze",
    "build": "turbo run build",
    "build:all": "pnpm db:generate && pnpm -r run build",
    "build:ci": "./scripts/build-local.sh",
    "check:architecture": "node scripts/check-circular-deps.js",
    "check:builds": "node scripts/check-builds.js",
    "check:ports": "node scripts/check-ports.js",
    "clean": "turbo run clean",
    "clean:all": "pnpm -r run clean",
    "dev": "turbo run dev",
    "dev:env": "node -r dotenv/config scripts/dev-with-env.js",
    "dev:trainer": "node -r dotenv/config scripts/dev-trainer-with-env.js",
    "dev:web": "node -r dotenv/config scripts/dev-web-with-env.js",
    "format": "prettier --cache --write .",
    "format:check": "prettier --check .",
    "preinstall": "only-allow pnpm",
    "lint": "turbo run lint",
    "lint:all": "pnpm -r run lint",
    "lint:fix": "eslint --cache --fix \"{apps,packages}/**/*.{js,ts,tsx}\"",
    "optimize:all": "pnpm optimize:imports && pnpm optimize:deps && pnpm analyze:web",
    "optimize:deps": "node scripts/remove-unused-deps.js",
    "optimize:imports": "node scripts/optimize-imports.js",
    "setup:env": "node scripts/setup-env.js",
    "start": "turbo run start --parallel",
    "start:all": "node scripts/start-all.js",
    "start:basic": "node scripts/start-basic.js",
    "start:stable": "node scripts/start-stable.js",
    "test": "turbo run test",
    "test:all": "pnpm -r run test",
    "typecheck": "turbo run typecheck",
    "typecheck:all": "pnpm -r run typecheck",
    "typecheck:ci": "./scripts/typecheck-local.sh"
  },
  "dependencies": {
    "dotenv": "17.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.32.0",
    "@types/node": "20.4.8",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@types/web-push": "3.6.4",
    "eslint": "9.32.0",
    "eslint-config-next": "15.4.5",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "3.10.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "15.15.0",
    "next": "15.3.5",
    "only-allow": "1.2.1",
    "prettier": "3.6.2",
    "prettier-plugin-packagejson": "2.5.19",
    "prettier-plugin-tailwindcss": "0.6.14",
    "rimraf": "5.0.1",
    "tsx": "4.19.0",
    "turbo": "2.5.4",
    "typescript": "5.8.3",
    "typescript-eslint": "8.39.0"
  },
  "packageManager": "pnpm@10.12.4",
  "pnpm": {
    "overrides": {
      "@mui/icons-material": "7.1.0",
      "@mui/material": "7.1.0",
      "@mui/x-data-grid": "7.8.0",
      "@mui/x-date-pickers": "7.8.0",
      "@types/react": "19.1.9",
      "@types/react-dom": "19.1.7",
      "cross-spawn@<6.0.6": ">=6.0.6",
      "esbuild@<=0.24.2": ">=0.25.0",
      "form-data@<2.5.4": ">=2.5.4",
      "got@<11.8.5": ">=11.8.5",
      "http-cache-semantics@<4.1.1": ">=4.1.1",
      "next": "15.3.5",
      "next-auth": "4.24.11",
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "semver-regex@<3.1.3": ">=3.1.3",
      "semver-regex@<3.1.4": ">=3.1.4",
      "tough-cookie@<4.1.3": ">=4.1.3"
    }
  }
}
