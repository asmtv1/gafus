{
  "name": "gafus.ru",
  "version": "1.0.0",
  "private": true,
  "description": "Комплексная система управления фитнес-тренировками с веб-приложением, панелью тренера, телеграм-ботом и системой мониторинга",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "analyze:dependencies": "tsx refactor/analyze-dependencies.ts",
    "analyze:trainer": "pnpm --filter @gafus/trainer-panel analyze",
    "analyze:web": "pnpm --filter @gafus/web analyze",
    "benchmark:baseline": "node scripts/benchmark-baseline.js",
    "build": "turbo run build",
    "build:all": "pnpm db:generate && pnpm -r run build",
    "build:ci": "./scripts/build-local.sh",
    "build:packages": "pnpm --filter \"./packages/**\" run build",
    "check:all": "pnpm check:prisma-transactions && pnpm check:getCurrentUserId && pnpm check:csrf && pnpm check:formdata && pnpm check:cache && pnpm check:transactions-files && pnpm check:api-routes",
    "check:api-routes": "tsx refactor/check-existing-api-routes.ts",
    "check:builds": "node scripts/check-builds.js",
    "check:cache": "tsx refactor/check-unstable-cache-usage.ts",
    "check:csrf": "tsx refactor/check-csrf-in-api-routes.ts",
    "check:formdata": "tsx refactor/check-formdata-conversion.ts",
    "check:getCurrentUserId": "tsx refactor/check-getCurrentUserId-in-api-routes.ts",
    "check:ports": "node scripts/check-ports.js",
    "check:prisma-transactions": "tsx refactor/check-prisma-transactions-timeouts.ts",
    "check:transactions-files": "tsx refactor/check-transactions-with-files.ts",
    "clean": "turbo run clean",
    "clean:all": "pnpm -r run clean",
    "clean:cache": "bash scripts/clean-cache.sh",
    "dev": "turbo run dev",
    "dev:env": "node -r dotenv/config scripts/dev-with-env.js",
    "find:unused": "node scripts/find-unused-functions.js",
    "format": "prettier --cache --write .",
    "format:check": "prettier --check .",
    "generate:openapi": "node scripts/generate-openapi-simple.js",
    "generate:server-actions-docs": "node scripts/generate-server-actions-docs.js",
    "preinstall": "only-allow pnpm",
    "lint": "turbo run lint",
    "lint:all": "pnpm -r run lint",
    "lint:fix": "eslint --cache --fix \"{apps,packages}/**/*.{js,ts,tsx}\"",
    "optimize:all": "pnpm analyze:web",
    "prepare:refactor": "pnpm analyze:dependencies && pnpm benchmark:baseline && pnpm test:characterization && pnpm check:all",
    "ready-for-refactor": "pnpm prepare:refactor && pnpm build",
    "setup:env": "node scripts/setup-env.js",
    "start": "node scripts/start-all.js",
    "start:all": "node scripts/start-all.js",
    "start:worker:watch:simple": "nodemon",
    "test": "turbo run test",
    "test:all": "pnpm -r run test",
    "test:characterization": "tsx characterization-tests/training-characterization.test.ts && tsx characterization-tests/course-characterization.test.ts && tsx characterization-tests/notifications-characterization.test.ts",
    "test:errors": "node scripts/generate-test-errors.js",
    "test:errors:all": "node scripts/generate-test-errors.js",
    "typecheck": "turbo run typecheck",
    "typecheck:all": "pnpm -r run typecheck"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.899.0",
    "@aws-sdk/s3-request-presigner": "^3.899.0",
    "@gafus/cdn-upload": "workspace:^",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.85.9",
    "@tanstack/react-query-devtools": "^5.85.9",
    "dotenv": "17.2.0",
    "sweetalert2": "^11.23.0"
  },
  "devDependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.32.0",
    "@tanstack/query-sync-storage-persister": "^5.85.9",
    "@tanstack/react-query-persist-client": "^5.85.9",
    "@types/node": "20.4.8",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@types/web-push": "3.6.4",
    "eslint": "9.32.0",
    "eslint-config-next": "15.5.7",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "3.10.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "15.15.0",
    "next": "15.5.7",
    "nodemon": "^3.0.1",
    "only-allow": "1.2.1",
    "prettier": "3.6.2",
    "prettier-plugin-packagejson": "2.5.19",
    "prettier-plugin-tailwindcss": "0.6.14",
    "rimraf": "5.0.1",
    "tsx": "4.19.0",
    "turbo": "2.5.4",
    "typescript": "5.8.3",
    "typescript-eslint": "8.46.2",
    "vitest": "^4.0.17",
    "zod-to-json-schema": "^3.24.6"
  },
  "packageManager": "pnpm@10.12.4",
  "pnpm": {
    "overrides": {
      "@mui/icons-material": "7.1.0",
      "@mui/material": "7.1.0",
      "@mui/x-data-grid": "7.8.0",
      "@mui/x-date-pickers": "7.8.0",
      "@prisma/client": "6.10.1",
      "@types/react": "19.1.9",
      "@types/react-dom": "19.1.7",
      "cross-spawn@<6.0.6": ">=6.0.6",
      "esbuild@<=0.24.2": ">=0.25.0",
      "form-data@<2.5.4": ">=2.5.4",
      "got@<11.8.5": ">=11.8.5",
      "http-cache-semantics@<4.1.1": ">=4.1.1",
      "next": "15.5.7",
      "next-auth": "4.24.11",
      "prisma": "6.10.1",
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "semver-regex@<3.1.3": ">=3.1.3",
      "semver-regex@<3.1.4": ">=3.1.4",
      "tough-cookie@<4.1.3": ">=4.1.3",
      "zod": "^3.24.1"
    }
  }
}
