      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(pg_database_size_bytes)",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 10737418240, "color": "yellow"},
              {"value": 53687091200, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "textMode": "value_and_name",
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 5,
      "title": "Cache Hit Ratio",
      "type": "gauge",
      "gridPos": {"h": 8, "w": 8, "x": 0, "y": 4},
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(pg_stat_database_blks_hit) / (sum(pg_stat_database_blks_hit) + sum(pg_stat_database_blks_read)) * 100",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 80, "color": "yellow"},
              {"value": 95, "color": "green"}
            ]
          }
        }
      },
      "options": {
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
