{
  "id": null,
  "uid": "postgres-metrics",
  "title": "PostgreSQL метрики",
  "tags": ["postgres", "database"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 0,
  "refresh": "30s",
  "editable": true,
  "panels": [
    {
      "id": 1,
      "title": "Статус PostgreSQL",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "pg_up",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {"text": "Офлайн", "color": "red"},
                "1": {"text": "Онлайн", "color": "green"}
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 1, "color": "green"}
            ]
          }
        }
      },
      "options": {
        "textMode": "value_and_name",
        "colorMode": "value",
        "graphMode": "none"
      }
    },
    {
      "id": 2,
      "title": "Время отклика",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "avg(pg_stat_activity_max_conn_time) * 1000",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 100, "color": "yellow"},
              {"value": 500, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "textMode": "value_and_name",
        "colorMode": "value",
        "graphMode": "area"
      }
    },
