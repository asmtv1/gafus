{
  "dashboard": {
    "title": "Очереди BullMQ",
    "tags": ["bullmq", "queues"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Задания в очереди",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "bullmq_queue_waiting_total",
            "legendFormat": "{{queue}} - Waiting",
            "refId": "A"
          },
          {
            "expr": "bullmq_queue_active_total",
            "legendFormat": "{{queue}} - Active",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "title": "Завершенные задания",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "rate(bullmq_queue_completed_total[5m])",
            "legendFormat": "{{queue}} - Completed/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        }
      },
      {
        "id": 3,
        "title": "Неудачные задания",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(bullmq_queue_failed_total[5m])",
            "legendFormat": "{{queue}} - Failed/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        }
      },
      {
        "id": 4,
        "title": "Время обработки заданий",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "bullmq_job_duration_seconds",
            "legendFormat": "{{queue}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      }
    ]
  }
}

