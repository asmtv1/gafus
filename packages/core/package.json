{
  "name": "@gafus/core",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/core/src/index.js",
  "types": "./dist/core/src/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/core/src/index.d.ts",
      "default": "./dist/core/src/index.js"
    },
    "./services/course": {
      "types": "./dist/core/src/services/course/index.d.ts",
      "default": "./dist/core/src/services/course/index.js"
    },
    "./services/user": {
      "types": "./dist/core/src/services/user/index.d.ts",
      "default": "./dist/core/src/services/user/index.js"
    },
    "./services/auth": {
      "types": "./dist/core/src/services/auth/index.d.ts",
      "default": "./dist/core/src/services/auth/index.js"
    },
    "./services/notifications": {
      "types": "./dist/core/src/services/notifications/index.d.ts",
      "default": "./dist/core/src/services/notifications/index.js"
    },
    "./services/subscriptions": {
      "types": "./dist/core/src/services/subscriptions/index.d.ts",
      "default": "./dist/core/src/services/subscriptions/index.js"
    },
    "./services/tracking": {
      "types": "./dist/core/src/services/tracking/index.d.ts",
      "default": "./dist/core/src/services/tracking/index.js"
    },
    "./services/achievements": {
      "types": "./dist/core/src/services/achievements/index.d.ts",
      "default": "./dist/core/src/services/achievements/index.js"
    },
    "./services/pets": {
      "types": "./dist/core/src/services/pets/index.d.ts",
      "default": "./dist/core/src/services/pets/index.js"
