    "./services/training": {
      "types": "./dist/core/src/services/training/index.d.ts",
      "default": "./dist/core/src/services/training/index.js"
    },
    "./errors": {
      "types": "./dist/core/src/errors/index.d.ts",
      "default": "./dist/core/src/errors/index.js"
    },
    "./utils": {
      "types": "./dist/core/src/utils/index.d.ts",
      "default": "./dist/core/src/utils/index.js"
    },
    "./utils/social": {
      "types": "./dist/core/src/utils/social.d.ts",
      "default": "./dist/core/src/utils/social.js"
    },
    "./utils/training": {
      "types": "./dist/core/src/utils/training.d.ts",
      "default": "./dist/core/src/utils/training.js"
    },
    "./utils/retry": {
      "types": "./dist/core/src/utils/retry.d.ts",
      "default": "./dist/core/src/utils/retry.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@gafus/prisma": "workspace:*",
    "@gafus/logger": "workspace:*",
    "@gafus/types": "workspace:*",
    "@gafus/queues": "workspace:*",
    "@gafus/cdn-upload": "workspace:*",
    "@gafus/auth": "workspace:*",
    "zod": "^3.24.1",
    "libphonenumber-js": "^1.12.8"
  },
  "devDependencies": {
    "@types/node": "20.4.8",
    "rimraf": "5.0.1",
    "typescript": "5.8.3"
  }
