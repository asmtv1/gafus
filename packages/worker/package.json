{
  "name": "@gafus/worker",
  "version": "0.1.0",
  "private": true,
  "main": "dist/worker/src/index.js",
  "types": "dist/worker/src/index.d.ts",
  "scripts": {
    "prebuild": "pnpm clean",
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "dev": "tsx --watch src/index.ts",
    "dev:env": "tsx --env-file=../../.env src/index.ts",
    "start": "node dist/worker/src/index.js",
    "start:dev": "tsx src/index.ts",
    "start:watch": "nodemon dist/worker/src/index.js --watch dist --ext js --ignore dist/**/*.map --exec 'node'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@gafus/cdn-upload": "workspace:*",
    "@gafus/logger": "workspace:*",
    "@gafus/prisma": "workspace:*",
    "@gafus/queues": "workspace:*",
    "@gafus/reengagement": "workspace:*",
    "@gafus/types": "workspace:*",
    "@gafus/webpush": "workspace:*",
    "bullmq": "5.2.0",
    "expo-server-sdk": "^3.15.0",
    "ioredis": "5.3.2",
    "node-cron": "^3.0.3",
    "web-push": "3.6.6"
  },
  "devDependencies": {
    "@types/node": "20.4.8",
    "@types/node-cron": "^3.0.11",
    "@types/web-push": "3.6.4",
    "rimraf": "5.0.1",
    "tsx": "4.19.0",
    "typescript": "5.8.3"
  }
}
